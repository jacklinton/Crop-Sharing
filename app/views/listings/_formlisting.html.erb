<%= simple_form_for @listing do |f| %>
    <%= f.input :title, required:true  %>
    <%= f.input :address, required:true, input_html: { class: "address" } %>
    <%= f.input :category %>
    <%= f.input :acc_cash %>
    <%= f.input :acc_trade %>
    <%= f.input :description %>
    <%= f.input :wants %>
    <%= f.input :quantity %>
    <%= f.input :harvest_date, as: :date, html5: true %>
    <%= f.input :exp_date, as: :date, html5: true %>
    <%= f.input :picture, as: :file %>
    <%= f.button :submit, class: "btn btn-primary" %>
<% end %>